<Window x:Class="UdemyAssignment52.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UdemyAssignment52"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Label Content="Num1"/>
        <TextBox Width="100" Height="30" Text="{Binding Path=Num1, Mode=TwoWay}"/>
        <Label Content="Num2"/>
        <TextBox Width="100" Height="30" Text="{Binding Path=Num2, Mode=TwoWay}"/>
        <Label Content="Add result"/>
        <TextBox Width="100" Height="30" Text="{Binding Path=SumResult, Mode=TwoWay}"/>
        <Label Content="Multiply result"/>
        <TextBox Width="100" Height="30" Text="{Binding Path=MultiResult, Mode=TwoWay}"/>
    </StackPanel>
</Window>
